<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.game">

    <application
        android:allowBackup="true"
        android:icon="@drawable/carotty"
        android:label="TAMZ II Game"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.game.LoseActivity"></activity>
        <activity android:name="com.example.game.ChooseLevelActivity" />
        <activity android:name="com.example.game.HeroActivity" />
        <activity android:name="com.example.game.ScoreBoardActivity" />
        <activity android:name="com.example.game.WinScreenActivity" />
        <activity android:name="com.example.game.MainActivity" />
        <activity android:name="com.example.game.MenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>